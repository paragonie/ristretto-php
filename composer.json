{
  "name": "paragonie/ristretto",
  "description": "Type-safe API for the Ristretto255 group",
  "license": "ISC",
  "type": "library",
  "authors": [
    {
      "name": "Paragon Initiative Enterprises",
      "email": "security@paragonie.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ParagonIE\\Ristretto\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParagonIE\\Ristretto\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "paragonie/constant_time_encoding": "^2",
    "paragonie/hidden-string": "^2",
    "paragonie/sodium_compat": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vimeo/psalm": "^4"
  },
  "suggest": {
    "ext-sodium": "Better performance."
  }
}